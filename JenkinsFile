pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                echo 'Building..'
                echo 'Buindling test done'
                echo 'one more test'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
                echo 'Test Done'
            }
        }
        stage('Deploy') {
            steps {
                echo 'deployment'
                echo 'one more test code'
            }
        }
    }
}
